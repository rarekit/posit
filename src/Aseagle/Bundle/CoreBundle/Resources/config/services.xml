<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="aseagle.ftp" class="Aseagle\Bundle\CoreBundle\Service\Ftp">
            <argument type="service" id="service_container" />
        </service>
        <service id="aseagle.acl" class="Aseagle\Bundle\CoreBundle\Service\Acl">
            <argument type="service" id="service_container" />
        </service>
        <service id="aseagle.image" class="Aseagle\Bundle\CoreBundle\Service\AseagleImage" />
    </services>   
</container>
